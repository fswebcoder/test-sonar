{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "aurum_suite": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "",
            "sourceRoot": "public",
            "prefix": "svi",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:application",
                    "options": {
                        "outputPath": "dist/aurum_suite",
                        "index": "src/index.html",
                        "browser": "src/main.ts",
                        "polyfills": [
                            "zone.js"
                        ],
                        "tsConfig": "tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "public"
                            },
                            {
                                "glob": "**/*",
                                "input": "src/assets",
                                "output": "assets"
                            }
                        ],
                        "styles": [
                            "src/styles.scss",
                            "node_modules/primeflex/primeflex.css"
                        ],
                        "scripts": [],
                        "allowedCommonJsDependencies": [
                            "crypto-js",
                            "lodash"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "4kb",
                                    "maximumError": "8kb"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "optimization": false,
                            "extractLicenses": false,
                            "sourceMap": true
                        },
                        "svi-dev": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "4kb",
                                    "maximumError": "8kb"
                                }
                            ],
                            "outputHashing": "all",
                            "fileReplacements": [
                                {
                                    "replace": "src/enviromments/environment.ts",
                                    "with": "src/enviromments/environment.svi-dev.ts"
                                }
                            ]
                        },
                        "svi-test": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "4kb",
                                    "maximumError": "8kb"
                                }
                            ],
                            "outputHashing": "all",
                            "fileReplacements": [
                                {
                                    "replace": "src/enviromments/environment.ts",
                                    "with": "src/enviromments/environment.svi-test.ts"
                                }
                            ]
                        },
                        "svi-pdn": {
                            "optimization": true,
                            "sourceMap": false,
                            "aot": true,
                            "extractLicenses": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "4kb",
                                    "maximumError": "8kb"
                                }
                            ],
                            "outputHashing": "all",
                            "fileReplacements": [
                                {
                                    "replace": "src/enviromments/environment.ts",
                                    "with": "src/enviromments/environment.svi-pdn.ts"
                                }
                            ]
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "buildTarget": "aurum_suite:build:production"
                        },
                        "development": {
                            "buildTarget": "aurum_suite:build:development"
                        },
                        "svi-dev": {
                            "buildTarget": "aurum_suite:build:svi-dev"
                        },
                        "svi-test": {
                            "buildTarget": "aurum_suite:build:svi-test"
                        },
                        "svi-pdn": {
                            "buildTarget": "aurum_suite:build:svi-pdn"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n"
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "polyfills": [
                            "zone.js",
                            "zone.js/testing"
                        ],
                        "tsConfig": "tsconfig.spec.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "public"
                            }
                        ],
                        "styles": [
                            "src/styles.scss"
                        ],
                        "scripts": []
                    }
                }
            }
        },
        "utilities": {
          "projectType": "library",
          "root": "projects/utilities",
          "sourceRoot": "projects/utilities/src",
          "prefix": "lib",
          "architect": {
            "build": {
              "builder": "@angular-devkit/build-angular:ng-packagr",
              "options": {
                "project": "projects/utilities/ng-package.json"
              },
              "configurations": {
                "production": {
                  "tsConfig": "projects/utilities/tsconfig.lib.prod.json"
                },
                "development": {
                  "tsConfig": "projects/utilities/tsconfig.lib.json"
                }
              },
              "defaultConfiguration": "production"
            },
            "test": {
              "builder": "@angular-devkit/build-angular:karma",
              "options": {
                "tsConfig": "projects/utilities/tsconfig.spec.json",
                "polyfills": [
                  "zone.js",
                  "zone.js/testing"
                ]
              }
            }
          }
        }
    },
    "cli": {
      "analytics": "a226201c-7751-471a-b98b-f34414588426"
    }
}
